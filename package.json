{
  "name": "bookcafe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "npm run dev:backend & npm run dev:frontend",
    "dev:backend": "cross-env NODE_ENV=development DEBUG=express:* node-dev ./bin/dev",
    "dev:frontend": "webpack-dev-server --inline --progress --config frontend/build/webpack.dev.conf.js",
    "lint": "eslint --ext .js,.vue src",
    "build": "node frontend/build/build.js",
    "scan": "node ./backend/scripts/scan.js",
    "convert": "node ./backend/scripts/convert.js",
    "clear": "node ./backend/scripts/clear.js",
    "reset": "node ./backend/scripts/reset.js",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.15.1",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "corser": "^2.0.1",
    "cross-env": "^5.1.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "express-session": "^1.15.6",
    "imagemagick-stream": "^4.1.1",
    "lodash": "^4.17.4",
    "mongoose": "^5.0.0-rc1",
    "morgan": "~1.7.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "~2.3.0",
    "uuid": "^3.2.1",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.0.0-beta.40",
    "@vue/cli-plugin-babel": "^3.0.0-beta.1",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.1",
    "@vue/cli-service": "^3.0.0-beta.1",
    "animejs": "^2.2.0",
    "axios": "^0.17.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-dev": "^3.1.3",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.7.2",
    "postcss-import": "^11.0.0",
    "random-color": "^1.0.1",
    "sass-loader": "^6.0.6",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^0.5.8",
    "vue-lazyload": "^1.1.4",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
